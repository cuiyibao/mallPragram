/**index.wxss**/

.menu-wrp {
    width:100%;
    margin-top:20rpx;
}
.menu-wrp:after{
    content:"";
    display:block;
    clear:both;
}
.menu-list{
    float:left;
    width:20%;
    box-sizing: border-box;
    padding-bottom:10px;
}
.menu-img{
    width:120rpx;
    height:84rpx;
    display:block;
    margin:0 auto;
    margin-bottom:5px;
}
.menu-desc{
    background-color:#ffffff;
    color:#333333;
    width:100%;
    text-align: center;
    display:block;
    font-size:12px;
}